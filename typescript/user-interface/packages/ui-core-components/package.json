{
  "name": "@gms/ui-core-components",
  "version": "0.1.0",
  "description": "GMS UI Core Components",
  "author": "GMS Development Team",
  "license": "UNLICENSED",
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "main": "./lib/ui-core-components.js",
  "module": "./lib/ui-core-components.js",
  "typings": "./lib/ui-core-components.d.ts",
  "jest-junit": {
    "suiteNameTemplate": "ui-core-components/{filename}",
    "classNameTemplate": "ui-core-components/{filename}/{classname}",
    "titleTemplate": "{title}"
  },
  "scripts": {
    "clean:node_modules": "rimraf node_modules/",
    "clean:lib": "rimraf lib/",
    "clean:dist": "rimraf dist/ && rimraf bundle-analyzer/",
    "clean:coverage": "rimraf coverage/",
    "clean:docs": "rimraf docs/",
    "clean": "npm-run-all -p clean:lib clean:dist clean:coverage clean:docs -p clean:node_modules",
    "build:eslint": "yarn eslint -c .eslintrc-config.yaml 'src/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache",
    "build:eslint:test": "yarn eslint -c .eslintrc-config-test.yaml '{__tests__,__mocks__,__integration-tests__}/**/*.{ts,tsx}' --quiet --cache --cache-file .eslintcache-test",
    "build:tsc-defs": "tsc --project ./tsconfig.json --emitDeclarationOnly",
    "build:tsc:dev": "tsc --project ./tsconfig.json --sourceMap true --declaration true --declarationMap true",
    "build:tsc:prod": "tsc --project ./tsconfig.json --sourceMap false --declaration true --declarationMap false",
    "build:tsc:test": "tsc --project ./tsconfig-test.json",
    "build:dev": "npm-run-all -p clean:lib -p build:tsc:dev",
    "build:prod": "npm-run-all -p clean:lib -p build:tsc:prod",
    "build": "yarn build:dev",
    "build:webpack:dev": "webpack --colors --env.development --config webpack.config.ts",
    "build:webpack:prod": "webpack --colors --env.production --config webpack.config.ts",
    "bundle:dev": "npm-run-all -p clean:dist -p build:webpack:dev",
    "bundle:prod": "npm-run-all -p clean:dist -p build:webpack:prod",
    "bundle": "yarn bundle:dev",
    "dev": "webpack-dev-server --colors --progress --env.development --env.devserver --config webpack.config.ts",
    "start": "webpack-dev-server --colors --progress --env.production --env.devserver --config webpack.config.ts",
    "watch": "yarn build:tsc:dev --watch --preserveWatchOutput",
    "docs": "yarn clean:docs && typedoc --out docs ./src/ --name ui-core-components --mode file --readme README.md --target ES6 --includeDeclarations --excludeExternals --excludeNotExported --hideGenerator",
    "sonar": "echo \"run 'yarn sonar' from the top-level directory\"",
    "test": "yarn test:dev",
    "test:dev:jest": "jest --config jest.config.js --silent false --verbose=true --bail=true",
    "test:prod:jest": "jest --config jest.config.js --verbose=false --bail=true --coverageReporters=lcovonly --reporters=default --reporters=jest-junit",
    "test:dev": "npm-run-all -p build:tsc:test test:dev:jest",
    "test:prod": "npm-run-all -p build:tsc:test test:prod:jest",
    "version": "echo $npm_package_version"
  },
