{
  "name": "@gms/cypress-tests",
  "version": "0.1.0",
  "description": "GMS Cypress Tests",
  "author": "GMS Development Team",
  "license": "UNLICENSED",
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "scripts": {
    "clean:node_modules": "rimraf node_modules/",
    "clean": "npm-run-all -p clean:node_modules",
    "build:eslint:test": "yarn eslint 'cypress/**/*.{ts,tsx,js}' --quiet --cache --cache-file .eslintcache",
    "build:tsc:test": "tsc --project ./tsconfig.json",
    "test-cypress": "yarn cypress open",
    "test-cypress:headless": "yarn cypress-tags run",
    "test-cypress:ian": "yarn cypress-tags run -e TAGS=\"@ian and not @skip\"",
    "test-cypress:soh": "yarn cypress-tags run -e TAGS=\"@soh and not @skip\"",
    "test-cypress-firefox": "yarn cypress run --browser firefox",
    "report:merge": "mochawesome-merge ./dist/report/json/mochawesome*.json -o ./dist/report/json/report.json",
    "report:generate:html": "marge ./dist/report/json/report.json -o ./dist/report/html",
    "report:copyScreenshots": "cp -r ./dist/screenshots ./dist/report/html/screenshots 2>/dev/null || :",
    "report": "yarn report:merge && yarn report:generate:html && yarn report:copyScreenshots"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "commonPath": "cypress-tests/cypress/integration/cucumber/common"
  },
  "dependencies": {
    "@blueprintjs/core": "3.21.0",
    "@gms/common-model": "0.1.0",
    "@gms/common-util": "0.1.0",
    "@gms/ui-state": "0.1.0",
    "cypress-cucumber-preprocessor": "4.0.1",
    "cypress-plugin-snapshots": "1.4.4",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.21",
    "mochawesome": "6.1.1",
    "react-dom": "16.12.0",
    "react": "16.12.0",
    "react-redux": "7.2.2",
    "redux": "4.0.4"
  },
  "devDependencies": {
    "@blueprintjs/eslint-plugin": "0.3.2",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@types/react-redux": "7.1.5",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "cypress-cucumber-preprocessor": "4.0.1",
    "cypress-multi-reporters": "1.4.0",
    "cypress": "6.3.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsdoc": "30.7.9",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "24.0.0",
    "eslint-plugin-workspaces": "0.6.2",
    "mocha-junit-reporter": "2.0.0",
    "mocha": "8.2.0",
    "mochawesome-report-generator": "5.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "typedoc": "0.15.3",
    "typescript": "4.2.4",
    "webpack": "4.41.2"
  }
}
